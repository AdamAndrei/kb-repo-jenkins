			excelFileName="AWC2406_Part.xlsx"	
		ElseIf lcase(sObjType)="sales document" Then
			excelFileName="AWC2406_Sales Document.xlsx"	
		ElseIf lcase(sObjType)="create change request" Then
			excelFileName="AWC2406_ChangeRequest Document.xlsx"	
		ElseIf lcase(sObjType)="create sir" Then
			excelFileName="AWC2406_SIR Document.xlsx"			
		End If
		sColumnNm= sHeaderText&"_Add_"&Now		
		 folderPath= getTempFolder()
		 excelFilePath = folderPath & "\" & excelFileName
		call Fn_FSO_Perfomance_Monitor_Operation("AddNewColumn",folderPath,excelFileName,"Add",sColumnNm,"")
		'--------------------------------------------------------------------------------------------------------------------------------
		'Click on Settings -> Performace Monitor
		'--------------------------------------------------------------------------------------------------------------------------------
		If  Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Creation_Operation ","Click",obj_AWCTeamcenterHome,"wele_UserSessionBar_Settings","","","")  =False Then
			Reporter.ReportEvent micFail, "Click on [ Settings ] on primary toolbar", "Fail to click on [ Settings ] on primary toolbar"
			ExitComponent
		Else
			Reporter.ReportEvent micPass, "Click on [ Settings ] on primary toolbar", "Successfully clicked on [ Settings ] on primary toolbar"
		End If
		Call  Fn_AWC_ReadyStatusSync(1)
		
		Call Fn_AWC_Common_Business_Primary_Toolbar_Operations(obj_AWCTeamcenterHome,"Monitoring","Performance Monitor")
		Call  Fn_AWC_ReadyStatusSync(1)
		ExitComponent
	End If
		'--------------------------------------------------------------------------------------------------------------------------------	
		If  lcase(sObjType)<> "instrument" and lcase(sObjType)<> "create checklist template" and lcase(sObjType)<> "create mcr"and lcase(sObjType)<> "create change request" and lcase(sObjType)<> "create change proposal" and lcase(sObjType)<> "create sir"  and lcase(sObjType)<> "tr document" and lcase(sObjType)<> "part" and lcase(sObjType)<> "note"and lcase(sObjType)<> "tex document" and lcase(sObjType)<> "manufacturing document" and lcase(sObjType)<> "engineering document" and lcase(sObjType)<> "sales document" and lcase(sObjType)<> "private folder"and lcase(sObjType)<> "project folder" and lcase(sObjType)<> "release folder" Then
			'--------------------------------------------------------------------------------------------------------------------------------
			If instr(sStatusMesaage,sID)>0 Then
				Reporter.ReportEvent micPass, "Fn_AWC_Object_Creation_Operation", "Successfully Fn_AWC_Object_Creation_Operation as notification message  [ "&  sStatusMesaage  & " ] contains id ] " & sID
			Else
				Reporter.ReportEvent micFail, "Fn_AWC_Object_Creation_Operation", "Fail to Fn_AWC_Object_Creation_Operation as [ Notification message does not contains id ]" & sID
				ExitComponent
			End If	
			'--------------------------------------------------------------------------------------------------------------------------------			
		ElseIf lcase(sObjType)= "part" Then
			'--------------------------------------------------------------------------------------------------------------------------------
			If instr(sStatusMesaage,"created successfully")>0 Then
				Reporter.ReportEvent micPass, "Verify Notification message", "Successfully verified notification message which is [ "&  sStatusMesaage  & " ]"
				'If multiple parts = true
				'--------------------------------------------------------------------------------------------------------------------------------
				If lcase(bCreateMulParts) = "true" Then
					sTemp = split(sStatusMesaage,"""")
					sTemp1 =Split(sTemp(1),", ")
					sID = Join(sTemp1,";")
					'--------------------------------------------------------------------------------------------------------------------------------
					For iCounter = 0 To Ubound(sTemp1)
						sTemp2= sTemp1(iCounter)&",000"
						'--------------------------------------------------------------------------------------------------------------------------------
						If iCounter>0 Then
							sTemp3=sTemp3&";"&sTemp2
						else
							sTemp3=sTemp2
						End If
						'--------------------------------------------------------------------------------------------------------------------------------
					Next
					Fn_AWC_Object_Creation_Operation = sTemp3 & "~" & sID
					'--------------------------------------------------------------------------------------------------------------------------------
				Else
					sTemp = split(sStatusMesaage,"""")
					sID =sTemp(1)
					Fn_AWC_Object_Creation_Operation = sTemp(1) & ",000~" & sID
				End If
				'--------------------------------------------------------------------------------------------------------------------------------
			Else
				Reporter.ReportEvent micFail, "Verify Notification message", "Fail to Create the KB_MANUFACTURING object as [ Fail to verify notification message ]"
				ExitComponent
			End If	
			'--------------------------------------------------------------------------------------------------------------------------------
		End  IF
'	End  If
	sTempValue=split(sStatusMesaage,"""")
	'--------------------------------------------------------------------------------------------------------------------------------
	'Store created object value
	'--------------------------------------------------------------------------------------------------------------------------------
	If lcase(sObjType)="sales document"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="engineering document"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="create mcr"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="instrument"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="create sir"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="create change proposal"  Then
		sOjectNodeName=split(sTempValue(1),"/")(0)
		sTempNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="create checklist template"  Then
		sOjectNodeName=split(sTempValue(1),"/")(0)
		sTempNodeName=sTempValue(1)	
	ElseIf lcase(sObjType)="create change request"  Then
		sTempNodeName= Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "getroproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectHeader_2"), "", "innertext", "")
		sOjectNodeName=sTempNodeName
	ElseIf lcase(sObjType)="tr document"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="tex document"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="manufacturing document"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf  lcase(sObjType)="private folder" Then
		sOjectNodeName=sFolderName & "," & sFolderDesc
	ElseIf  lcase(sObjType)="release folder"Then	
		sTempNodeName=sTempValue(1)
		sTemp = split(sStatusMesaage, "/")
		sTempValue=len(sTemp(0))
		sID = mid(sTemp(0),2,sTempValue)
		sOjectNodeName=sID
	ElseIf lcase(sObjType)="note"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf lcase(sObjType)="transfer folder"  Then
		sOjectNodeName=sTempValue(1)
	ElseIf  lcase(sObjType)="project folder" Then
		sOjectNodeName=sFolderDesc & "," & sFolderName
	End If  
	'--------------------------------------------------------------------------------------------------------------------------------
	If lcase(sObjType)= "sales document" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sID
	ElseIf lcase(sObjType)= "engineering document" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sID
	ElseIf lcase(sObjType)= "create mcr" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName
	ElseIf lcase(sObjType)= "instrument" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName
	ElseIf lcase(sObjType)= "create sir" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName
	ElseIf lcase(sObjType)= "create change proposal" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sTempNodeName
	ElseIf lcase(sObjType)= "create checklist template" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sTempNodeName
	ElseIf lcase(sObjType)= "create change request" Then
		sID=split(sOjectNodeName,"/")(0)
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sID
	ElseIf lcase(sObjType)= "tr document" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sID
	ElseIf lcase(sObjType)= "tex document" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sID
	ElseIf lcase(sObjType)= "manufacturing document" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sID
	ElseIf  lcase(sObjType)="private folder"Then	
		Fn_AWC_Object_Creation_Operation=sFolderName & "," & sFolderDesc
	ElseIf  lcase(sObjType)="project folder"Then	
		Fn_AWC_Object_Creation_Operation=sFolderDesc & "," & sFolderName
	ElseIf  lcase(sObjType)="release folder" Then
		Fn_AWC_Object_Creation_Operation = sOjectNodeName & "~" & sTempNodeName
	ElseIf  lcase(sObjType)="note" Then
		Fn_AWC_Object_Creation_Operation=sOjectNodeName
	ElseIf  lcase(sObjType)="transfer folder" Then
		Fn_AWC_Object_Creation_Operation = sID  & "~" & sID
	End If
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	If Err.Number<> 0 Then
		Reporter.ReportEvent micFail, "Fn_AWC_Object_Creation_Operation", "Fail to perform [ Fn_AWC_Object_Creation_Operation ]  Operation due to [ " & Err.Description & " ]"
		ExitComponent
	Else
		Reporter.ReportEvent micPass, "Fn_AWC_Object_Creation_Operation", "Successfully performed [ Fn_AWC_Object_Creation_Operation ] operation "
	End If
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	'Set object nothing
	Set obj_AWCTeamcenterHome=Nothing
	Set objWshShell=Nothing
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
End Function
'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------@Function Header Start 
'Function Name				:	Fn_AWC_Object_SaveAs_Operation
'Function Description		:	Function used to create an object
'Function Parameters		: 	 1.sObjType : Business Object Type
'								 2.sProObj : Primary Object ro save as
'								3.sdic : Dictionary variable to store the property name and property value on creation dialog
'								4.sPerformance : Performance monitor
'Function Return Value	   	: 	Newly Saved Object
'Wrapper Function	    	 	: 	NA
'Function Pre-requisite	 	:	AWC should be open and Primary Object is visible in navigation list
'Function Usage		   	 :	Call  Fn_AWC_Object_SaveAs_Operation(Parameter("str_object_type") ,Parameter("str_PrimaryObject") ,objDic,Parameter("str_Performance") )
'History			     			:
'----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
'	Developer Name			 |	Date			 |	      Reviewer			
'----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
'	Mohini Deshmukh		   	|  20 Jan 2025	    	|	 				 
'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------@Function Header End
Public Function Fn_AWC_Object_SaveAs_Operation(sObjType,sProObj,sdic,sPerformance)
	Err.Clear
	'-------------------------------------------------------------------------------------------------------------------------------
	'Variable Declaration
	'--------------------------------------------------------------------------------------------------------------------------------
	Dim obj_AWCTeamcenterHome
	Dim sNewObj,sTempProperites,sTempPropertyValues,sPropertyName,sPropertyValue,sXpath,sStatusMesaage,sTempValue,sTempPropertyValue
	Dim sTempLocation,sTempMakeBuy
	Dim iPropertyCount,iCounter,jCounter
	'--------------------------------------------------------------------------------------------------------------------------------
	'Get AWC PLM window object from xml
	'--------------------------------------------------------------------------------------------------------------------------------
	Set obj_AWCTeamcenterHome=Eval(SearchAndLoadResourceByName("ActiveWorkspace2406_OR.xml").GetValue("wpage_AWCTeamcenterHome"))
	'--------------------------------------------------------------------------------------------------------------------------------
	'Select the Item from navigation list
	'--------------------------------------------------------------------------------------------------------------------------------
	If sProObj <>"" Then
		Call Fn_AWC_Object_Navigation_Operations(obj_AWCTeamcenterHome,"select", sProObj)
	End IF
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	'Click the Save as or Revise  toolbar button and Select Save as or Revise -> Save As menu
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_SaveAs_Operation", "click", obj_AWCTeamcenterHome, "wbtn_SaveAsorRevise","","","")
	Call Fn_AWC_Common_Business_Primary_Toolbar_Operations(obj_AWCTeamcenterHome,gSaveAs,"")
	'--------------------------------------------------------------------------------------------------------------------------------	
	'Enter property values on Save As dialog		
	iPropertyCount=sdic.Count
	sTempProperites=sdic.Keys
	sTempPropertyValues=sdic.items
	'--------------------------------------------------------------------------------------------------------------------------------
	For iCounter=0 to iPropertyCount-1
		sPropertyName=sTempProperites(iCounter)
		sPropertyValue=sTempPropertyValues(iCounter)
		'--------------------------------------------------------------------------------------------------------------------------------
		Select Case lcase(sPropertyName)
		'--------------------------------------------------------------------------------------------------------------------------------
			Case "language","team","use status","branded for","saleable only to customer(s)","sales channel limitation"," sales index"
				sXpath=Replace(gEditBoxXpath,"~",sPropertyName)
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
				'--------------------------------------------------------------------------------------------------------------------------------
				If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome, "wedit_ObjectPropertyEditBox_2", "" ) Then
					sTempPropertyValue=Split(sPropertyValue,"~")
					'--------------------------------------------------------------------------------------------------------------------------------
					For jCounter = 0 To ubound(sTempPropertyValue)
						'--------------------------------------------------------------------------------------------------------------------------------
						If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValuePanel"), 10,2) Then
							If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_SaveAs_Operation ","set",obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "", trim(sTempPropertyValue(jCounter))) Then
								Reporter.ReportEvent micPass, "Type [ " & sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box", "Successfully typed " &  sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box"
							Else
								Reporter.ReportEvent micFail, "Type [ " & sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box", "Fail to type [ " & sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box"
								ExitComponent
							End  If
							'--------------------------------------------------------------------------------------------------------------------------------
							Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), "2", "innertext", sTempPropertyValue(jCounter))
							 If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), 10,2)  Then
								Call Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"),"","","","")
								Reporter.ReportEvent micPass, "Select the  " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field", "Successfully Selected the " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field"
							Else
								Reporter.ReportEvent micFail, "Select the  " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field", "Fail to Select the  " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field"
								ExitComponent
							End If	
							'--------------------------------------------------------------------------------------------------------------------------------							
						Else
							Reporter.ReportEvent micFail, "Enter " & sTempPropertyValue(jCounter) & " value into the " & sPropertyName & " edit box", "Fail to save as object of type  " & sObjType & " as [ " & sTempPropertyValue(jCounter) & " value is incorrect ]"
							ExitComponent
						End  If
						'--------------------------------------------------------------------------------------------------------------------------------
					Next
				Else
					Reporter.ReportEvent micFail, "Click in  " & sPropertyName & " list field", "Fail to Click in " & sPropertyName & " list field as [  List does not exist ]"
					ExitComponent
				End  IF	
			'--------------------------------------------------------------------------------------------------------------------------------
			'Select Projects
			Case "projects"	
				'--------------------------------------------------------------------------------------------------------------------------------
				'Click on Add Project button 
				'--------------------------------------------------------------------------------------------------------------------------------
				If Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_SaveAs_Operation", "Click", obj_AWCTeamcenterHome, "wbtn_AddProject","","","") =False Then
					Reporter.ReportEvent micFail, "Click on [ Add Project ] button ", "Fail to click on [ Add Project ] button"
					ExitComponent
				Else
					Reporter.ReportEvent micPass, "Click on [ Add Project ] button", "Successfully clicked on [ Add Project ] button"
				End If
				Call  Fn_AWC_ReadyStatusSync(2)
				'--------------------------------------------------------------------------------------------------------------------------------
				'Select Project from list 
				'--------------------------------------------------------------------------------------------------------------------------------
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_Project"), "2", "innertext", sPropertyValue )
				If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome,"wele_Project",1,1,micLeftBtn) =False Then
					Reporter.ReportEvent micFail, "Select the Project from list", "Fail to Select the Project from list"
					ExitComponent
				Else
					Reporter.ReportEvent micPass, "Select the Project from list", "Successfully Selected the  " & sPropertyValue & "  Project"
				End If
				'--------------------------------------------------------------------------------------------------------------------------------
				'Click on Assign Project button 
				'--------------------------------------------------------------------------------------------------------------------------------
				If Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_SaveAs_Operation", "Click", obj_AWCTeamcenterHome, "wbtn_AssignProjects","","","") =False Then
					Reporter.ReportEvent micFail, "Click on [ Assign ] button ", "Fail to click on [ Assign ] button"
					ExitComponent
				Else
					Reporter.ReportEvent micPass, "Click on [ Assign ] button", "Successfully clicked on [ Assign ] button"
				End If
				Call  Fn_AWC_ReadyStatusSync(2)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on checkbox
			Case "secure?","batch tracing"	
				sXpath=Replace(gCommonCheckBox2,"~",sPropertyName)
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("welechk_CommonCheckBox"), "2", "xpath", sXpath)
				'--------------------------------------------------------------------------------------------------------------------------------
				IF WaitUntilExist(obj_AWCTeamcenterHome.WebElement("welechk_CommonCheckBox"), 60, 5) Then
					Call Fn_Web_UI_WebCheckBox_Operations("Fn_AWC_Object_SaveAs_Operation","set",obj_AWCTeamcenterHome,"welechk_CommonCheckBox",sPropertyName,sPropertyValue)
					Call Fn_AWC_ReadyStatusSync(1)		
				End  IF	
			'--------------------------------------------------------------------------------------------------------------------------------	
			'Update location,make/buy indicator
			Case "location-make/buy indicator"
				
				sTempPropertyValue=Split(sPropertyValue,"*")
				
				sTempLocation=Split(sTempPropertyValue(0),"~")
				sTempMakeBuy=Split(sTempPropertyValue(1),"~")
				
				For jCounter = 0 to Ubound(sTempLocation)
					sXpath=Replace(gEditBoxXpath,"~",gLocation)
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
					'--------------------------------------------------------------------------------------------------------------------------------
					If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome, "wedit_ObjectPropertyEditBox_2", "" ) Then
						'--------------------------------------------------------------------------------------------------------------------------------
						If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValuePanel"), 10,2) Then
							'--------------------------------------------------------------------------------------------------------------------------------
							If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_SaveAs_Operation ","set",obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "", trim(sTempLocation(jCounter))) Then
