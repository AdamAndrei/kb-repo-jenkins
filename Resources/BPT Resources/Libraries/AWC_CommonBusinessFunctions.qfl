Option Explicit
Err.Clear
'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
'001.Fn_AWC_Object_Creation_Operation						|	Mohini.Deshmukh@knorr-bremse.com		|	14-Aug-2024	|	Function used to perform  create object
'002.Fn_AWC_Object_SaveAs_Operation						|	Mohini.Deshmukh@knorr-bremse.com		|	20-Jan-2025	|	Function used to perform  save as operation
'003.Fn_AWC_Object_Copy_To_Existing_Operation				|	Mohini.Deshmukh@knorr-bremse.com		|	27-Jan-2025	|	Function used to perform  copy to existing operation
'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------@Function Header Start 
'Function Name				:	Fn_AWC_Object_Creation_Operation
'Function Description		:	Function used to create an object
'Function Parameters		:   1.sAction : Create object by tile or by primary toolbar
'								2.sObjType : Business Object Type
'								3.sdic : Dictionary variable to store the property name and property value on creation dialog
'								4.sPerformance : Performance monitor
'Function Return Value	   	: 	Search Result
'Wrapper Function	    	 	: 	NA
'Function Pre-requisite	 	:	AWC should be open
'Function Usage		   	 :	Fn_AWC_Object_Creation_Operation("bytile",Parameter("str_type_of_object") ,objDic,Parameter("str_Performance") )
'History			     			:
'----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
'	Developer Name			 |	Date			 |	      Reviewer			
'----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
'	Mohini Deshmukh		   	|  14 Aug 2024	    	|	 				 
'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------@Function Header End
Public Function Fn_AWC_Object_Creation_Operation(sAction,sObjType,sdic,sPerformance)
	Err.Clear
	'-------------------------------------------------------------------------------------------------------------------------------
	'Variable Declaration
	'--------------------------------------------------------------------------------------------------------------------------------
	Dim obj_AWCTeamcenterHome,objWshShell
	Dim sLang,sStatusMesaage,sID,sOjectNodeName,sXpath,sTempPropertyValue,sTempProperites,sTempPropertyValues,sPropertyName,sPropertyValue,sSalesID
	Dim sTempLocation,sTempMakeBuy,sTempAction,sTemp,sTemp1,sTemp2,sTemp3,sTempProperty,sTempValue,sFolderName,sFolderDesc,sTempNodeName
	Dim iCounter,jCounter,iPropertyCount,sColumnNm,sSite,excelFileName,folderPath,excelFilePath,iCountParts
	Dim bAllocateID,bCreateMulParts
	Dim sQuestionTopic,sHeaderText
	'--------------------------------------------------------------------------------------------------------------------------------
	'Get AWC PLM window object from xml
	'--------------------------------------------------------------------------------------------------------------------------------
	Set obj_AWCTeamcenterHome=Eval(SearchAndLoadResourceByName("ActiveWorkspace2406_OR.xml").GetValue("wpage_AWCTeamcenterHome"))
	'''--------------------------------------------------------------------------------------------------------------------------------
	'''Verify the header for performance Monitor
	'''--------------------------------------------------------------------------------------------------------------------------------
	If lcase(sPerformance)="true" Then
		If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_Env_Header"), 5,5) Then
			sHeaderText=obj_AWCTeamcenterHome.WebElement("wele_Env_Header").GetROProperty("outertext")
		End If
		'--------------------------------------------------------------------------------------------------------------------------------
		'Click on Settings -> Performace Monitor
		'--------------------------------------------------------------------------------------------------------------------------------
		Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_Property_Name_On_Performance_Monitor"), "2", "innertext", "Total Time")
		If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "exist", obj_AWCTeamcenterHome.WebElement("wele_Property_Name_On_Performance_Monitor"), "5", "", "")=False  Then
		
			If  Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Creation_Operation ","Click",obj_AWCTeamcenterHome,"wele_UserSessionBar_Settings","","","")  =False Then
				Reporter.ReportEvent micFail, "Click on [ Settings ] on User session bar", "Fail to click on [ Settings ] on User session bar"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Click on [ Settings ] on User session bar", "Successfully clicked on [ Settings ] on User session bar"
			End If
			Call  Fn_AWC_ReadyStatusSync(1)
			
			Call Fn_AWC_Common_Business_Primary_Toolbar_Operations(obj_AWCTeamcenterHome,"Monitoring","Performance Monitor")
			Wait 2
			'-----------------------------------------------------------------------------------------------------------------------------------------------------------
			'Verify Performance Monitor Panel is open
			'-----------------------------------------------------------------------------------------------------------------------------------------------------------
			Call Fn_Web_UI_WebObject_Operations("Validate the Icons at the bottom - CAD user - AUTHOR Client", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_Property_Name_On_Performance_Monitor"), "2", "innertext", "Total Time")
			If Fn_Web_UI_WebElement_Operations(" Fn_Common_Business_Primary_Toolbar_Operations","Click",obj_AWCTeamcenterHome,"wele_Property_Name_On_Performance_Monitor","","","")=False  Then
				Reporter.ReportEvent micFail, "Verify [ Performance Monitor ] Panel is open.", "Fail to Verify [ Performance Monitor ] Panel is open."
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Verify [ Performance Monitor ] Panel is open.", "Successfully verified [ Performance Monitor ] Panel is open."
			End If	
		End If	
	End If
	'Select method to create object
	Select case lcase(sAction)
		'--------------------------------------------------------------------------------------------------------------------------------
		'Case to create object by tile
		Case "bytile"
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Home Folder
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Creation_Operation", "click", obj_AWCTeamcenterHome, "wbtn_Home","","","")
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Home page tile Create
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_HomePage_Tile"), "", "innertext", glblCREATE)
			If obj_AWCTeamcenterHome.WebElement("wele_HomePage_Tile").exist Then
				If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Creation_Operation","Click",obj_AWCTeamcenterHome,"wele_HomePage_Tile","","","") =False Then
					Reporter.ReportEvent micFail, "Click on [ "&glblCREATE&" ] Tile on home page", "Fail to click on [ "&glblCREATE&" ] Tile on home page"
					ExitComponent
				Else
					Reporter.ReportEvent micPass, "Click on [ "&glblCREATE&" ] Tile on home page", "Successfully clicked on [ "&glblCREATE&" ] Tile on home page"
				End If
			End  IF	
			Call  Fn_AWC_ReadyStatusSync(1)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Type Object type value in edit box
			'--------------------------------------------------------------------------------------------------------------------------------
			If obj_AWCTeamcenterHome.WebEdit("wedit_SearchBoxLOV").exist Then
				If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_Creation_Operation","Set",obj_AWCTeamcenterHome, "wedit_SearchBoxLOV", sObjType ) Then
					Reporter.ReportEvent micPass, "Type [ " & sObjType & " ] in Edit Box", "Successfully Typed [ " & sObjType & " ] in Edit Box"
				Else
					Reporter.ReportEvent micFail, "Type [ " & sObjType & " ] in Edit Box", "Fail to Type [ " & sObjType & " ] in Edit Box"
					ExitComponent
				End  If
			End If
			Call Fn_AWC_ReadyStatusSync(1)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Select Object type value in edit box
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectType"), "2", "innertext", sObjType)
			If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Creation_Operation","Click",obj_AWCTeamcenterHome,"wele_ObjectType","","","") =False Then
				Reporter.ReportEvent micFail, "Select object type", "Fail to select object type"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Select object type", "Successfully Selected object type is a [ " & sObjType & " ]"
			End If
			Call Fn_AWC_ReadyStatusSync(2)
		'--------------------------------------------------------------------------------------------------------------------------------
		'Case to create object by primary toolbar
		Case "by primary toolbar"
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Home Folder
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Creation_Operation", "click", obj_AWCTeamcenterHome, "wbtn_Home","","","")
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Explorer
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Creation_Operation", "click", obj_AWCTeamcenterHome, "wbtn_Explorer","","","")
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on more command on Primary tab
			'--------------------------------------------------------------------------------------------------------------------------------
			If  Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Creation_Operation ","Click",obj_AWCTeamcenterHome,"wele_WorkAreaTool_MoreCommand","","","")  =False Then
				Reporter.ReportEvent micFail, "Click on [ More Command ] on primary toolbar", "Fail to click on [ More Command ] on primary toolbar"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Click on [ More Command ] on primary toolbar", "Successfully clicked on [ More Command ] on primary toolbar"
			End If
			Call  Fn_AWC_ReadyStatusSync(1)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on New -> Add New
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_AWC_Common_Business_Primary_Toolbar_Operations(obj_AWCTeamcenterHome,gNew,gAdd)
			Call  Fn_AWC_ReadyStatusSync(1)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Type Object type value in edit box
			'--------------------------------------------------------------------------------------------------------------------------------
			If obj_AWCTeamcenterHome.WebEdit("wedit_SearchBoxLOV").exist Then
				If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_Creation_Operation","Set",obj_AWCTeamcenterHome, "wedit_SearchBoxLOV", sObjType ) Then
					Reporter.ReportEvent micPass, "Type [ " & sObjType & " ] in Edit Box", "Successfully Typed [ " & sObjType & " ] in Edit Box"
				Else
					Reporter.ReportEvent micFail, "Type [ " & sObjType & " ] in Edit Box", "Fail to Type [ " & sObjType & " ] in Edit Box"
					ExitComponent
				End  If
			End If
			Call Fn_AWC_ReadyStatusSync(1)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Select Object type value in edit box
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectType"), "2", "innertext", sObjType)
			If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Creation_Operation","Click",obj_AWCTeamcenterHome,"wele_ObjectType","","","") =False Then
				Reporter.ReportEvent micFail, "Select object type", "Fail to select object type"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Select object type", "Successfully Selected object type is a [ " & sObjType & " ]"
			End If
			Call Fn_AWC_ReadyStatusSync(2)
		'--------------------------------------------------------------------------------------------------------------------------------		
		'This action is to create object from Add To on work area panel
		Case "add to" 
		'	'--------------------------------------------------------------------------------------------------------------------------------
			'Type Object type value in edit box
			'--------------------------------------------------------------------------------------------------------------------------------
			If obj_AWCTeamcenterHome.WebEdit("wedit_SearchBoxLOV").exist Then
				If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_Creation_Operation","Set",obj_AWCTeamcenterHome, "wedit_SearchBoxLOV", sObjType ) Then
					Reporter.ReportEvent micPass, "Type [ " & sObjType & " ] in Edit Box", "Successfully Typed [ " & sObjType & " ] in Edit Box"
				Else
					Reporter.ReportEvent micFail, "Type [ " & sObjType & " ] in Edit Box", "Fail to Type [ " & sObjType & " ] in Edit Box"
					ExitComponent
				End  If
			End If
			Call Fn_AWC_ReadyStatusSync(1)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Select Object type value in edit box
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectType"), "2", "innertext", sObjType)
			If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Creation_Operation","Click",obj_AWCTeamcenterHome,"wele_ObjectType","","","") =False Then
				Reporter.ReportEvent micFail, "Select object type", "Fail to select object type"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Select object type", "Successfully Selected object type is a [ " & sObjType & " ]"
			End If
			Call Fn_AWC_ReadyStatusSync(2)
		'--------------------------------------------------------------------------------------------------------------------------------
		'Case to Create Change  object by primary toolbar
		Case "change object"
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Home Folder
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Creation_Operation", "click", obj_AWCTeamcenterHome, "wbtn_Home","","","")
			Call Fn_AWC_ReadyStatusSync(2)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Explorer
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Creation_Operation", "click", obj_AWCTeamcenterHome, "wbtn_Explorer","","","")
			Call Fn_AWC_ReadyStatusSync(2)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Create Change Objects button on Primary tab
			'--------------------------------------------------------------------------------------------------------------------------------
			If Fn_WEB_UI_WebButton_Operations("Create an object", "Click", obj_AWCTeamcenterHome, "wbtn_Create_Change_Objects","","","") =False Then
				Reporter.ReportEvent micFail, "Click on [ Create Change Objects ] toolbar button on primary toolbar", "Fail to click on [ Create Change Objects ] toolbar button on primary toolbar"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Click on [ Create Change Objects ] toolbar button on primary toolbar", "Successfully clicked on [ Create Change Objects ] toolbar button on primary toolbar"
			End If
			Call  Fn_AWC_ReadyStatusSync(1)
			'------------------------------------------------------------------------------------------------------------------------------
			' Select provided change object type
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_Web_UI_WebObject_Operations("Create an object", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_NewSubmenu"), "2", "innertext", sObjType)
			If Fn_Web_UI_WebElement_Operations("Create an object","Click",obj_AWCTeamcenterHome,"wele_NewSubmenu","","","") =False Then
				Reporter.ReportEvent micFail, "Click on [ " & cstr(sObjType) & " ] toolbar button on primary toolbar", "Fail to click on [ " & cstr(sObjType) & " ] toolbar button on primary toolbar"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Click on [ " & cstr(sObjType) & " ] toolbar button on primary toolbar", "Successfully clicked on [ " & cstr(sObjType) & " ] toolbar button on primary toolbar"
			End If
			Call Fn_AWC_ReadyStatusSync(2)
			'--------------------------------------------------------------------------------------------------------------------------------
			'Verify Create Change Proposal Panel is open
			'--------------------------------------------------------------------------------------------------------------------------------
			If lcase(sObjType) = "create mcr" Then
				sXpath=Replace(gChangeObjectsSubmenu,"~","Create Material Concession Request")
			ElseIf lcase(sObjType) = "create change proposal" Then
				sXpath=Replace(gChangeObjectsSubmenu,"~","Create Change")
			Else
				sXpath=Replace(gChangeObjectsSubmenu,"~",sObjType)
			End If
			Call Fn_Web_UI_WebObject_Operations("Create an object", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_Create_DocReq_Panel"), "2", "xpath", sXpath)
			If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_Create_DocReq_Panel"), 2, 5)=False Then
				Reporter.ReportEvent micFail, "Verify Panel [ " & sObjType & "] ", "Fail to verify [ " & sObjType & "] Panel is open"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Verify Panel [ " & sObjType & "]", "Successfully verified [ " & sObjType & "] Panel is opened"
			End  IF
			Call Fn_AWC_ReadyStatusSync(2)
		'--------------------------------------------------------------------------------------------------------------------------------
		'Case to Part , Add New Saleable Item by primary toolbar
		Case "create part","add new saleable item"
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Home Folder
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Creation_Operation", "click", obj_AWCTeamcenterHome, "wbtn_Home","","","")
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Explorer
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Creation_Operation", "click", obj_AWCTeamcenterHome, "wbtn_Explorer","","","")
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Create Part button on Primary tab
			'--------------------------------------------------------------------------------------------------------------------------------
			If Fn_WEB_UI_WebButton_Operations("Create an object", "Click", obj_AWCTeamcenterHome, "wbtn_Create_Part","","","") =False Then
				Reporter.ReportEvent micFail, "Click on [ Create Part ] toolbar button on primary toolbar", "Fail to click on [ Create Part ] toolbar button on primary toolbar"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Click on [ Create Part ] toolbar button on primary toolbar", "Successfully clicked on [ Create Part ] toolbar button on primary toolbar"
			End If
			Call  Fn_AWC_ReadyStatusSync(1)
			'--------------------------------------------------------------------------------------------------------------------------------
			If lcase(sAction)= "add new saleable item" Then
				'--------------------------------------------------------------------------------------------------------------------------------
				'Click on add new saleable item menu
				'--------------------------------------------------------------------------------------------------------------------------------
				Call Fn_Web_UI_WebObject_Operations("Create an object", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_NewSubmenu"), "2", "innertext", gAddNewSaleableItem)
				If Fn_Web_UI_WebElement_Operations("Create an object","Click",obj_AWCTeamcenterHome,"wele_NewSubmenu","","","") =False Then
					Reporter.ReportEvent micFail, "Click on [ " & cstr(gAddNewSaleableItem) & " ] toolbar button on primary toolbar", "Fail to click on [ " & cstr(gAddNewSaleableItem) & " ] toolbar button on primary toolbar"
					ExitComponent
				Else
					Reporter.ReportEvent micPass, "Click on [ " & cstr(gAddNewSaleableItem) & " ] toolbar button on primary toolbar", "Successfully clicked on [ " & cstr(gAddNewSaleableItem) & " ] toolbar button on primary toolbar"
				End If
				Call Fn_AWC_ReadyStatusSync(2)
				'--------------------------------------------------------------------------------------------------------------------------------
			ElseIf lcase(sAction)= "create part" Then
				'--------------------------------------------------------------------------------------------------------------------------------
				'Click on Create Part menu
				'--------------------------------------------------------------------------------------------------------------------------------
				Call Fn_Web_UI_WebObject_Operations("Create an object", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_NewSubmenu"), "2", "innertext", gCreatePart)
				If Fn_Web_UI_WebElement_Operations("Create an object","Click",obj_AWCTeamcenterHome,"wele_NewSubmenu","","","") =False Then
					Reporter.ReportEvent micFail, "Click on [ " & cstr(gCreatePart) & " ] toolbar button on primary toolbar", "Fail to click on [ " & cstr(gCreatePart) & " ] toolbar button on primary toolbar"
					ExitComponent
				Else
					Reporter.ReportEvent micPass, "Click on [ " & cstr(gCreatePart) & " ] toolbar button on primary toolbar", "Successfully clicked on [ " & cstr(gCreatePart) & " ] toolbar button on primary toolbar"
				End If
				Call Fn_AWC_ReadyStatusSync(2)
				'--------------------------------------------------------------------------------------------------------------------------------
			End If
			
	End Select
	'--------------------------------------------------------------------------------------------------------------------------------
	Select case(lcase(sObjType))
		'--------------------------------------------------------------------------------------------------------------------------------
		Case "sales document","engineering document","manufacturing document","tex document"	
			'--------------------------------------------------------------------------------------------------------------------------------
			'Store the ID value
			'--------------------------------------------------------------------------------------------------------------------------------
			sXpath=Replace(gEditBoxXpath,"~",glblID)
			Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
			sID= Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "getroproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "value", "")
			If sID="" Then
				Reporter.ReportEvent micFail, "Store the ID value ", "Fail to Fn_AWC_Object_Creation_Operation as fail to get ID value"
				ExitComponent
			End If
			Call Fn_AWC_ReadyStatusSync(1)
		'--------------------------------------------------------------------------------------------------------------------------------
		Case "transfer folder"
			'--------------------------------------------------------------------------------------------------------------------------------
			'Store the Name value
			'--------------------------------------------------------------------------------------------------------------------------------
			sXpath=Replace(gNameValue,"~",gName)
			Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_MainInfoNameValue"), "2", "xpath", sXpath)
			sID= Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "getroproperty", obj_AWCTeamcenterHome.WebElement("wele_MainInfoNameValue"), "2", "title", "")
			If sID="" Then
				Reporter.ReportEvent micFail, "Store the ID value ", "Fail to Fn_AWC_Object_Creation_Operation as fail to get ID value"
				ExitComponent
			End If
			Call Fn_AWC_ReadyStatusSync(1)
		'--------------------------------------------------------------------------------------------------------------------------------
		Case  "part" ,"add new saleable item"
			'--------------------------------------------------------------------------------------------------------------------------------
			'Click on Main tab of Create Part Panel
			'--------------------------------------------------------------------------------------------------------------------------------
			Call Fn_Web_UI_WebObject_Operations("Create an object", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_AddPanelTabs"), "2", "innertext", "Main")
			If Fn_Web_UI_WebElement_Operations("Create an object","Click",obj_AWCTeamcenterHome,"wele_AddPanelTabs","","","") =False Then
				Reporter.ReportEvent micFail, "Click on [ Main ] tab of Create Part panel", "Fail to click on [ Main ] tab of Create Part panel"
				ExitComponent
			Else
				Reporter.ReportEvent micPass, "Click on [ Main ] tab of Create Part panel", "Successfully clicked on [ Main ] tab of Create Part panel"
			End If
			Call Fn_AWC_ReadyStatusSync(1)
		'--------------------------------------------------------------------------------------------------------------------------------
	End Select		
	'--------------------------------------------------------------------------------------------------------------------------------
	'Enter property values on creation dialog		
	iPropertyCount=sdic.Count
	sTempProperites=sdic.Keys
	sTempPropertyValues=sdic.items
	For iCounter=0 to iPropertyCount-1
		sPropertyName=sTempProperites(iCounter)
		sPropertyValue=sTempPropertyValues(iCounter)
		'--------------------------------------------------------------------------------------------------------------------------------
		Select Case lcase(sPropertyName)
		'--------------------------------------------------------------------------------------------------------------------------------
			'Select the value from list field with have search edit box
			Case "calibration database","engineer responsible","manager responsible","responsible tester","global or regional change","responsible location","Project2","ca owner 1","ca owner 2","ca owner 3","ca owner 4","ca owner 5","root cause 1","root cause 2","root cause 3","root cause 4","root cause 5","quality manager","location/plant","analyst","r/d manager","sqd","business type","unit of costs","customer affected","cr complexity","functional safety","functional safety manager","system","projects list","change owner","admin manager","crb leader","change manager","change template","weight unit (from r/d)","surface","material","default unit","material group","reman type","device code","article type","saleable item type","design owner","use status","correct product group","ip owner","type number","correct product group","cp analyst","cp manager","responsible pg","responsible location/plant","responsible global coc","priority","classification","category","reason","test classification","component code","test part disposition","test part status","test location","device code","customer","department code","requested by location","responsible coc","requested by","test status","billing type","test category","responsible department","scope object","scope","responsible process","process driver","team","cvs company name","language","branded for","responsible","language version owner","product group","folder name","name","title id"
				'--------------------------------------------------------------------------------------------------------------------------------			
				If lcase(sPropertyName)="projects list" Then
					sPropertyName="Projects"
				ElseIf lcase(sPropertyName)="Project2" Then
					sPropertyName="Project"
				End If	
				'Added this code for negative testing of crete part with reman type - 23 Jan 2025-by Mohini
				'--------------------------------------------------------------------------------------------------------------------------------
				If lcase(sPropertyName)="reman type" and lcase(sPropertyValue)="user is not in k_mod_addeditremaneng group" Then
					sXpath=Replace(gEditBoxXpath,"~",sPropertyName)
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
					'--------------------------------------------------------------------------------------------------------------------------------
					Call	 Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_Creation_Operation","Click",obj_AWCTeamcenterHome, "wedit_ObjectPropertyEditBox_2", "" ) 
				
					obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg").click
					sStatusMesaage= Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "getroproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg"), "2", "innertext", "")	
					obj_AWCTeamcenterHome.WebButton("wbtn_NotificationClose").Click
					If instr(sStatusMesaage,sPropertyValue)>0 Then
						Reporter.ReportEvent micPass, "Verify error message", "Successfully verified as error message [ " & sPropertyValue & " ]"
						Fn_AWC_Object_Creation_Operation=True
						Exit Function
					Else
						Reporter.ReportEvent micFail, "Verify error message", "Fail to verify error message [ " & sPropertyValue & " ]"
						ExitComponent
					End If
				End If
				'--------------------------------------------------------------------------------------------------------------------------------			
				If lcase(sPropertyName)="ca owner 1" or lcase(sPropertyName)="ca owner 2" or lcase(sPropertyName)="ca owner 3" or lcase(sPropertyName)="ca owner 4" or lcase(sPropertyName)="ca owner 5"	Then
					'--------------------------------------------------------------------------------------------------------------------------------
					'Click on Corrective Actions tab of Create MCR Panel
					'--------------------------------------------------------------------------------------------------------------------------------
					Call Fn_Web_UI_WebObject_Operations("Create an object", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_AddPanelTabs"), "2", "innertext", gtabCorrectiveActions)
					If Fn_Web_UI_WebElement_Operations("Create an object","Click",obj_AWCTeamcenterHome,"wele_AddPanelTabs","","","") =False Then
						Reporter.ReportEvent micFail, "Click on [ "&gtabCorrectiveActions&" ] tab of Create Part panel", "Fail to click on [ "&gtabCorrectiveActions&" ] tab of Create Part panel"
						ExitComponent
					Else
						Reporter.ReportEvent micPass, "Click on [ "&gtabCorrectiveActions&" ] tab of Create Part panel", "Successfully clicked on [ "&gtabCorrectiveActions&" ] tab of Create Part panel"
					End If
				End  IF	
				'--------------------------------------------------------------------------------------------------------------------------------				
				sXpath=Replace(gEditBoxXpath,"~",sPropertyName)
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Creation_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
				wait 2
				'--------------------------------------------------------------------------------------------------------------------------------
