							Else
								Reporter.ReportEvent micFail, "Type [ " & sTempLocation(jCounter) & " value in [ " & gLocation & " ] edit Box", "Fail to type [ " & sTempLocation(jCounter) & " value in [ " & gLocation & " ] edit Box"
								ExitComponent
							End  If
							'--------------------------------------------------------------------------------------------------------------------------------
							Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), "2", "innertext", sTempLocation(jCounter))
							 If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), 10,2)  Then
								Call Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"),"","","","")
								Reporter.ReportEvent micPass, "Select the  " & sTempLocation(jCounter) & " value from  " & gLocation & " list field", "Successfully Selected the " & sTempLocation(jCounter) & " value from  " & gLocation & " list field"
							Else
								Reporter.ReportEvent micFail, "Select the  " & sTempLocation(jCounter) & " value from  " & gLocation & " list field", "Fail to Select the  " & sTempLocation(jCounter) & " value from  " & gLocation & " list field"
								ExitComponent
							End If	
							'--------------------------------------------------------------------------------------------------------------------------------							
						Else
							Reporter.ReportEvent micFail, "Enter " & sTempLocation(jCounter) & " value into the " & gLocation & " edit box", "Fail to save as object of type  " & sObjType & " as [ " & sTempLocation(jCounter) & " value is incorrect ]"
							ExitComponent
						End  If
						'--------------------------------------------------------------------------------------------------------------------------------
					Else
						Reporter.ReportEvent micFail, "Click in  " & gLocation & " list field", "Fail to Click in " & gLocation & " list field as [  List does not exist ]"
						ExitComponent
					End  IF		
					'--------------------------------------------------------------------------------------------------------------------------------
					'Select Make/Buy Indicator
					 sXpath=Replace(gEditBoxXpath,"~",gMakeBuyIndicator1)
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
					'--------------------------------------------------------------------------------------------------------------------------------
					If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome, "wedit_ObjectPropertyEditBox_2", "" ) Then
						'--------------------------------------------------------------------------------------------------------------------------------
						If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValuePanel"), 10,2) Then
							If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_SaveAs_Operation ","set",obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "", trim(sTempMakeBuy(jCounter))) Then
								Reporter.ReportEvent micPass, "Type [ " & sTempMakeBuy(jCounter) & " value in [ " & gMakeBuyIndicator1 & " ] edit Box", "Successfully typed " &  sTempMakeBuy(jCounter) & " value in [ " & gMakeBuyIndicator1 & " ] edit Box"
							Else
								Reporter.ReportEvent micFail, "Type [ " & sTempMakeBuy(jCounter) & " value in [ " & gMakeBuyIndicator1 & " ] edit Box", "Fail to type [ " & sTempMakeBuy(jCounter) & " value in [ " & gMakeBuyIndicator1 & " ] edit Box"
								ExitComponent
							End  If
							'--------------------------------------------------------------------------------------------------------------------------------
							Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), "2", "innertext", sTempMakeBuy(jCounter))
							 If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), 10,2)  Then
								Call Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"),"","","","")
								Reporter.ReportEvent micPass, "Select the  " & sTempMakeBuy(jCounter) & " value from  " & gMakeBuyIndicator1 & " list field", "Successfully Selected the " & sTempMakeBuy(jCounter) & " value from  " & gMakeBuyIndicator1 & " list field"
							Else
								Reporter.ReportEvent micFail, "Select the  " & sTempMakeBuy(jCounter) & " value from  " & gMakeBuyIndicator1 & " list field", "Fail to Select the  " & sTempMakeBuy(jCounter) & " value from  " & gMakeBuyIndicator1 & " list field"
								ExitComponent
							End If	
							'--------------------------------------------------------------------------------------------------------------------------------							
						Else
							Reporter.ReportEvent micFail, "Enter " & sTempMakeBuy(jCounter) & " value into the " & gMakeBuyIndicator1 & " edit box", "Fail to save as object of type  " & sObjType & " as [ " & sTempMakeBuy(jCounter) & " value is incorrect ]"
							ExitComponent
						End  If
						'--------------------------------------------------------------------------------------------------------------------------------
					Else
						Reporter.ReportEvent micFail, "Click in  " & gMakeBuyIndicator1 & " list field", "Fail to Click in " & gMakeBuyIndicator1 & " list field as [  List does not exist ]"
						ExitComponent
					End  IF	
					'--------------------------------------------------------------------------------------------------------------------------------
					'Click on Add Entry button 
					'--------------------------------------------------------------------------------------------------------------------------------
					If Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_SaveAs_Operation", "Click", obj_AWCTeamcenterHome, "wbtn_Add_Entry","","","") =False Then
						Reporter.ReportEvent micFail, "Click on [ Add Entry ] button ", "Fail to click on [ Add Entry ] button"
						ExitComponent
					Else
						Reporter.ReportEvent micPass, "Click on [ Add Entry ] button", "Successfully clicked on [ Add Entry ] button"
					End If
					Call  Fn_AWC_ReadyStatusSync(2)
					'--------------------------------------------------------------------------------------------------------------------------------	
				Next
				'--------------------------------------------------------------------------------------------------------------------------------
				'Select the radio button value	
				Case "default phantom part","hazardous material","under export control (military use possible)","contained in sales bom only?","software included?"
					sXpath=Replace(gCommonRadioButton2,"~",sPropertyName)
					sXpath=Replace(sXpath,"*",sPropertyValue)
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_CommonRadioButton_2"), "2", "xpath", sXpath)
					If  Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_SaveAs_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_CommonRadioButton_2"),"","","","") Then
					 Reporter.ReportEvent micPass, "Select the  " & sPropertyName & " value", "Successfully Selected the  " & sPropertyName & "  value is a [ " & sPropertyValue & " ]"
					ELse
						Reporter.ReportEvent micFail, "Select the " & sPropertyName & " value", "Fail to Select the " & sPropertyName & "  value"
						ExitComponent
					End  IF	
				'--------------------------------------------------------------------------------------------------------------------------------
				'Enter  value in text filed
				Case "software version code"
					sXpath=Replace(gEditBoxXpath,"~",sPropertyName)
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
					If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "waituntilexist", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "20000", "visible", "True") Then
						'--------------------------------------------------------------------------------------------------------------------------------
						IF Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_SaveAs_Operation","Set",obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "", trim(sPropertyValue)) Then
							Reporter.ReportEvent micPass, "Enter the value in [ "& sPropertyName &" ] field on Save As dialog", "Successfully entered the value [  "& sPropertyValue &" ] in [ " & sPropertyName & " ] field on Save As dialog "
						Else
							Reporter.ReportEvent micFail, "Enter the value in [ "& sPropertyName &" ] field on Save As dialog", "Fail to enter value [  "& sPropertyValue &" ] in [ " & sPropertyName & " ] field on Save As dialog"
							ExitComponent
						End  IF
						Call Fn_AWC_ReadyStatusSync(2)	
						'--------------------------------------------------------------------------------------------------------------------------------
					Else
						Reporter.ReportEvent micFail, "Verify existance of " & sPropertyName & " edit field.", sPropertyName & " edit field does not exist."
						ExitComponent		
					End If	
			'--------------------------------------------------------------------------------------------------------------------------------				
	         End  Select	
	'--------------------------------------------------------------------------------------------------------------------------------
	Next
	'--------------------------------------------------------------------------------------------------------------------------------
	'Click on Save As button 
	'--------------------------------------------------------------------------------------------------------------------------------
	If Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_SaveAs_Operation", "Click", obj_AWCTeamcenterHome, "wbtn_SaveAs_On_SaveAs_Dialog","","","") =False Then
		Reporter.ReportEvent micFail, "Click on [ Save As ] button ", "Fail to click on [ Save As ] button"
		ExitComponent
	Else
		Reporter.ReportEvent micPass, "Click on [ Save As ] button", "Successfully clicked on [ Save As ] button"
	End If
	'--------------------------------------------------------------------------------------------------------------------------------
	'Verify notification message
	'--------------------------------------------------------------------------------------------------------------------------------
	If obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg").WaitProperty("visible","true") Then
		obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg").Click
		sStatusMesaage= Fn_Web_UI_WebObject_Operations("Create a new object from an existing object via pirmary toolbar comand Save As or Revise - Save As", "getroproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg"), "2", "innertext", "")
		obj_AWCTeamcenterHome.WebButton("wbtn_NotificationClose").Click
		If instr( sStatusMesaage,"was saved")>0 Then
			Reporter.ReportEvent micPass, "Verify notification message", "Successfully verified notification message which is [ "&  sStatusMesaage  & " ]"
		Else
			Reporter.ReportEvent micFail, "Verify notification message" , "Failed to verify notification message as notification message is  " & sStatusMesaage 
			ExitComponent		
		End If	
	End  IF
	Call Fn_AWC_ReadyStatusSync(1)
	'--------------------------------------------------------------------------------------------------------------------------------
	'Click Versions-> Revision History tab
	'--------------------------------------------------------------------------------------------------------------------------------
	If lcase(sObjType)="part" Then
		Call Fn_AWC_Object_Navigation_WorkArea_Panel_Operations(obj_AWCTeamcenterHome,"selectview_in_section","Table",gVersion & "~" & gRevisionHistory)
	Else
		Call Fn_AWC_Object_Navigation_WorkArea_Panel_Operations(obj_AWCTeamcenterHome,"selectview_in_section","Table",gVersions & "~" & gRevisionHistory)
	End If
	'--------------------------------------------------------------------------------------------------------------------------------
	'Fetch newly saved object value
	'--------------------------------------------------------------------------------------------------------------------------------
	sNewObj= Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "getroproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectHeader_2"), "2", "innertext", "")
	'--------------------------------------------------------------------------------------------------------------------------------
	'Verify Newly Saved Object under Versions-> Revision History tab
	'--------------------------------------------------------------------------------------------------------------------------------
	Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_attachments_Added_Table_2"), "2", "innertext",sNewObj )
	If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "exist", obj_AWCTeamcenterHome.WebElement("wele_attachments_Added_Table_2"), "2", "", "") Then
		Reporter.ReportEvent micPass, "Verify [ "&sNewObj&" ] is available under  " & gRevisionHistory & " table list", "Successfully verified [ "&sNewObj&" ] is available under  " & gRevisionHistory & " table list"	
	Else
		Reporter.ReportEvent micFail, "Verify [ "&sNewObj&" ] is available under  " & gRevisionHistory & " table list", "Failed to verify [ "&sNewObj&" ] is available under  " & gRevisionHistory & " table list"
		ExitComponent	
	End If
	'--------------------------------------------------------------------------------------------------------------------------------
	'Click Versions-> Copied From tab
	'--------------------------------------------------------------------------------------------------------------------------------
	If lcase(sObjType)="part" Then
		Call Fn_AWC_Object_Navigation_WorkArea_Panel_Operations(obj_AWCTeamcenterHome,"selectview_in_section","Table",gVersion & "~" & gCopiedFrom)
	Else
		Call Fn_AWC_Object_Navigation_WorkArea_Panel_Operations(obj_AWCTeamcenterHome,"selectview_in_section","Table",gVersions & "~" & gCopiedFrom)
	End  IF	
	'--------------------------------------------------------------------------------------------------------------------------------
	'Verify Primary Object under Versions-> Copied From tab
	'--------------------------------------------------------------------------------------------------------------------------------
	Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_attachments_Added_Table_2"), "2", "innertext",sProObj )
	If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_SaveAs_Operation", "exist", obj_AWCTeamcenterHome.WebElement("wele_attachments_Added_Table_2"), "2", "", "") Then
		Reporter.ReportEvent micPass, "Verify [ "&sProObj&" ] is available under  " & gCopiedFrom & " table list", "Successfully verified [ "&sProObj&" ] is available under  " & gCopiedFrom & " table list"
	Else
		Reporter.ReportEvent micFail, "Verify ["&sProObj&"] is available under  " & gCopiedFrom & " table list", "Failed to verify [ "&sProObj&" ] is available under  " & gCopiedFrom & " table list"
		ExitComponent	
	End If
	'--------------------------------------------------------------------------------------------------------------------------------
	'Click  back button
	'--------------------------------------------------------------------------------------------------------------------------------
	Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_SaveAs_Operation", "click", obj_AWCTeamcenterHome, "wbtn_GoBack","","","")
	Call Fn_AWC_ReadyStatusSync(1)
	'--------------------------------------------------------------------------------------------------------------------------------
	Fn_AWC_Object_SaveAs_Operation=sNewObj
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	If Err.Number<> 0 Then
		Reporter.ReportEvent micFail, " Fn_AWC_Object_SaveAs_Operation", "Fail to perform [ Fn_AWC_Object_SaveAs_Operation ]  operation due to [ " & Err.Description & " ]"
		ExitComponent
	Else
		Reporter.ReportEvent micPass, "Fn_AWC_Object_SaveAs_Operation", "Successfully performed [ Fn_AWC_Object_SaveAs_Operation ] operation "
	End If
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	'Release the object 
	Set obj_AWCTeamcenterHome=Nothing
	'--------------------------------------------------------------------------------------------------------------------------------
End  Function
'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------@Function Header Start 
'Function Name				:	Fn_AWC_Object_Copy_To_Existing_Operation
'Function Description		:   Function used to copy to existiong operation from search and palette section
'Function Parameters		:   1.sObjectType : Object Type
'								2.sPriObject : Primary object 
'								3.sSecObject : Object to be search
'								4.sTabNm : Tab Name
'								5.sSecNm : Section Name
'								6.sPanelTitleNm : Paneltitle name either palette or Search
'								8.sdic : for properties on copy to existing dialog
'Function Return Value	   	: 	
'Wrapper Function	    	 	: 	NA
'Function Pre-requisite	 	:	AWC should be open and object should be selected
'Function Usage		   	: Call Fn_AWC_Object_Copy_To_Existing_Operation(Parameter("str_ObjectType") ,Parameter("str_PriObject") ,Parameter("str_SecObject") ,Parameter("str_TabName") ,Parameter("str_SecName") ,Parameter("str_PanelName") ,sdic)
'History			     			:
'----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
'	Developer Name				|	Date				|	      Reviewer			
'----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
'	Mohini Deshmukh		   	|  27 Jan 2025	    	|	 				 
'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------@Function Header End
Public Function Fn_AWC_Object_Copy_To_Existing_Operation(sObjectType,sPriObject,sSecObject,sTabNm,sSecNm,sPanelTitleNm,sdic)

	Err.Clear
	'-------------------------------------------------------------------------------------------------------------------------------
	'Variable Declaration
	'--------------------------------------------------------------------------------------------------------------------------------
	Dim obj_AWCTeamcenterHome
	Dim iCounter,iPropertyCount,jCounter
	Dim sTempPropertyValues,sTempProperites,sXpath,sPropertyValue,sPropertyName,sTempTab,sID,sStatusMesaage,sTempPropertyValue
	Fn_AWC_Object_Copy_To_Existing_Operation=False
	'--------------------------------------------------------------------------------------------------------------------------------
	'Get AWC PLM window object from xml
	'--------------------------------------------------------------------------------------------------------------------------------
	Set obj_AWCTeamcenterHome=Eval(SearchAndLoadResourceByName("ActiveWorkspace2406_OR.xml").GetValue("wpage_AWCTeamcenterHome"))
	'--------------------------------------------------------------------------------------------------------------------------------
	'Select the Item from navigation list
	'--------------------------------------------------------------------------------------------------------------------------------
	If sPriObject <>"" Then
		Call Fn_AWC_Object_Navigation_Operations(obj_AWCTeamcenterHome,"select", sPriObject)
	End IF
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	'Click the Save as or Revise  toolbar button and Select Save as or Revise -> Save As menu
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	Call Fn_WEB_UI_WebButton_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "click", obj_AWCTeamcenterHome, "wbtn_SaveAsorRevise","","","")
	Call Fn_AWC_Common_Business_Primary_Toolbar_Operations(obj_AWCTeamcenterHome,gCopyToExisting,"")
	'--------------------------------------------------------------------------------------------------------------------------------	
	'Enter property values on Save As dialog		
	iPropertyCount=sdic.Count
	sTempProperites=sdic.Keys
	sTempPropertyValues=sdic.items
	'--------------------------------------------------------------------------------------------------------------------------------
	For iCounter=0 to iPropertyCount-1
		sPropertyName=sTempProperites(iCounter)
		sPropertyValue=sTempPropertyValues(iCounter)
		'--------------------------------------------------------------------------------------------------------------------------------
		Select Case lcase(sPropertyName)
		'--------------------------------------------------------------------------------------------------------------------------------
			Case "team"
				sXpath=Replace(gEditBoxXpath,"~",sPropertyName)
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "2", "xpath", sXpath)
				'--------------------------------------------------------------------------------------------------------------------------------
				If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome, "wedit_ObjectPropertyEditBox_2", "" ) Then
					sTempPropertyValue=Split(sPropertyValue,"~")
					'--------------------------------------------------------------------------------------------------------------------------------
					For jCounter = 0 To ubound(sTempPropertyValue)
						'--------------------------------------------------------------------------------------------------------------------------------
						If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValuePanel"), 10,2) Then
							If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_Copy_To_Existing_Operation ","set",obj_AWCTeamcenterHome.WebEdit("wedit_ObjectPropertyEditBox_2"), "", trim(sTempPropertyValue(jCounter))) Then
								Reporter.ReportEvent micPass, "Type [ " & sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box", "Successfully typed " &  sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box"
							Else
								Reporter.ReportEvent micFail, "Type [ " & sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box", "Fail to type [ " & sTempPropertyValue(jCounter) & " value in [ " & sPropertyName & " ] edit Box"
								ExitComponent
							End  If
							'--------------------------------------------------------------------------------------------------------------------------------
							Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), "2", "innertext", sTempPropertyValue(jCounter))
							 If WaitUntilExist(obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"), 10,2)  Then
								Call Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_ObjectListValue"),"","","","")
								Reporter.ReportEvent micPass, "Select the  " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field", "Successfully Selected the " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field"
							Else
								Reporter.ReportEvent micFail, "Select the  " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field", "Fail to Select the  " & sTempPropertyValue(jCounter) & " value from  " & sPropertyName & " list field"
								ExitComponent
							End If	
							'--------------------------------------------------------------------------------------------------------------------------------							
						Else
							Reporter.ReportEvent micFail, "Enter " & sTempPropertyValue(jCounter) & " value into the " & sPropertyName & " edit box", "Fail to save as object of type  " & sObjType & " as [ " & sTempPropertyValue(jCounter) & " value is incorrect ]"
							ExitComponent
						End  If
						'--------------------------------------------------------------------------------------------------------------------------------
					Next
				Else
					Reporter.ReportEvent micFail, "Click in  " & sPropertyName & " list field", "Fail to Click in " & sPropertyName & " list field as [  List does not exist ]"
					ExitComponent
				End  IF	
			'--------------------------------------------------------------------------------------------------------------------------------
		End Select
	Next	
	'--------------------------------------------------------------------------------------------------------------------------------
	'Select panel tab either Palette or Search
	'--------------------------------------------------------------------------------------------------------------------------------
	If instr(lcase(sPanelTitleNm),"palette")>0 Then
		sTempTab=Split(sPanelTitleNm,"~")
		'--------------------------------------------------------------------------------------------------------------------------------
		'Click on Palette tab of Relate Panel
		'--------------------------------------------------------------------------------------------------------------------------------
		Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_AddPanelTabs"), "2", "innertext", gtabPalette)
		If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_AddPanelTabs"),"","","","")=False Then
			Reporter.ReportEvent micFail, "Click on [ "&gtabPalette&" ] tab of Add panel", "Fail to click on [ "&gtabPalette&" ] tab of Add panel"
			ExitComponent
		Else
			Reporter.ReportEvent micPass, "Click on [ "&gtabPalette&" ] tab of Add panel", "Successfully clicked on [ "&gtabPalette&" ] tab of Add panel"
		End If
		Call Fn_AWC_ReadyStatusSync(1)
		'--------------------------------------------------------------------------------------------------------------------------------
		'Select sections on Palette Panel
		'--------------------------------------------------------------------------------------------------------------------------------		
		Select Case lcase(sTempTab(1))
			'--------------------------------------------------------------------------------------------------------------------------------
			'Select Favorite section from palette	
			Case "favorites"
				'--------------------------------------------------------------------------------------------------------------------------------
				'Select object from favorites section if it is not already selected
				'--------------------------------------------------------------------------------------------------------------------------------
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_Selected_object_Favorites_Palette_Add_Panel"), "2", "innertext",sSecObject )
				If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "exist", obj_AWCTeamcenterHome.WebElement("wele_Selected_object_Favorites_Palette_Add_Panel"), "2", "","" )=False Then			
					'--------------------------------------------------------------------------------------------------------------------------------
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_object_Favorites_Palette_Add_Panel"), "2", "innertext",sSecObject )
					If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_object_Favorites_Palette_Add_Panel"),"","","","")=False Then
						Reporter.ReportEvent micFail, "Select [ "&sSecObject&" ] from  " & sTempTab(1)  & " section.", "Fail to Select [ "&sSecObject&" ] from " & sTempTab(1)  & " section"
						ExitComponent
					Else
						Reporter.ReportEvent micPass, "Select [ "&sSecObject&" ] from " & sTempTab(1)  & "  section", "Successfully Selected [ "&sSecObject&" ] from  " & sTempTab(1)  & "  section"
					End If
					'--------------------------------------------------------------------------------------------------------------------------------
				Else
					Reporter.ReportEvent micPass, "Select [ "&sSecObject&" ] from " & sTempTab(1)  & "  section", "Already [ "&sSecObject&"  ] is selected from  " & sTempTab(1)  & "  section"
				End  IF					
			'--------------------------------------------------------------------------------------------------------------------------------	
			'Select Clipboard section from palette	
			Case "clipboard"
				'--------------------------------------------------------------------------------------------------------------------------------
				'Select object from Clipboard section  if it is not already selected
				'--------------------------------------------------------------------------------------------------------------------------------
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_Selected_object_Clipboard_Palette_Add_Panel"), "2", "innertext",sSecObject )
				If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "exist", obj_AWCTeamcenterHome.WebElement("wele_Selected_object_Clipboard_Palette_Add_Panel"), "2", "","" )=False Then			
					'--------------------------------------------------------------------------------------------------------------------------------
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_object_Clipboard_Palette_Add_Panel"), "2", "innertext",sSecObject )
					If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_object_Clipboard_Palette_Add_Panel"),"","","","")=False Then
						Reporter.ReportEvent micFail, "Select [ "&sSecObject&" ] from  " & sTempTab(1)  & " section.", "Fail to Select [ "&sSecObject&" ] from " & sTempTab(1)  & " section"
						ExitComponent
					Else
						Reporter.ReportEvent micPass, "Select [ "&sSecObject&" ] from " & sTempTab(1)  & "  section", "Successfully Selected [ "&sSecObject&" ] from  " & sTempTab(1)  & "  section"
					End If
					'--------------------------------------------------------------------------------------------------------------------------------
				Else
					Reporter.ReportEvent micPass, "Select [ "&sSecObject&" ] from " & sTempTab(1)  & "  section", "Already [ "&sSecObject&"  ] is selected from  " & sTempTab(1)  & "  section"
				End  IF		
			'--------------------------------------------------------------------------------------------------------------------------------	
			'Select Recent section from palette	
			Case "recent"
				'--------------------------------------------------------------------------------------------------------------------------------
				'Select object from Recent section  if it is not already selected
				'--------------------------------------------------------------------------------------------------------------------------------
				Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_Selected_object_Recent_Palette_Add_Panel"), "2", "innertext",sSecObject )
				If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "exist", obj_AWCTeamcenterHome.WebElement("wele_Selected_object_Recent_Palette_Add_Panel"), "2", "","" )=False Then			
					'--------------------------------------------------------------------------------------------------------------------------------
					Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_object_Recent_Palette_Add_Panel"), "2", "innertext",sSecObject )
					If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_object_Recent_Palette_Add_Panel"),"","","","")=False Then
						Reporter.ReportEvent micFail, "Select [ "&sSecObject&" ] from  " & sTempTab(1)  & " section.", "Fail to Select [ "&sSecObject&" ] from " & sTempTab(1)  & " section"
						ExitComponent
					Else
						Reporter.ReportEvent micPass, "Select [ "&sSecObject&" ] from " & sTempTab(1)  & "  section", "Successfully Selected [ "&sSecObject&" ] from  " & sTempTab(1)  & "  section"
					End If
					'--------------------------------------------------------------------------------------------------------------------------------
				Else
					Reporter.ReportEvent micPass, "Select [ "&sSecObject&" ] from " & sTempTab(1)  & "  section", "Already [ "&sSecObject&"  ] is selected from  " & sTempTab(1)  & "  section"
				End  IF	
				'--------------------------------------------------------------------------------------------------------------------------------					
		End Select
		'--------------------------------------------------------------------------------------------------------------------------------		
	Else
		'--------------------------------------------------------------------------------------------------------------------------------
		'Click on Search tab of Relate Panel
		'--------------------------------------------------------------------------------------------------------------------------------
		Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_AddPanelTabs"), "2", "innertext", gtabSearch)
		
		If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_AddPanelTabs"),"","","","")=False Then
			Reporter.ReportEvent micFail, "Click on [ "&gtabSearch&" ] tab of Add panel", "Fail to click on [ "&gtabSearch&" ] tab of Add panel"
			ExitComponent
		Else
			Reporter.ReportEvent micPass, "Click on [ "&gtabSearch&" ] tab of Add panel", "Successfully clicked on [ "&gtabSearch&" ] tab of Add panel"
		End If
		Call Fn_AWC_ReadyStatusSync(1)	
		'--------------------------------------------------------------------------------------------------------------------------------
		'set criteria in Search edit box
		'--------------------------------------------------------------------------------------------------------------------------------
		sID=Split(sSecObject,",")(0)
		If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation ", "waituntilexist", obj_AWCTeamcenterHome.WebEdit("wedt_Search_Edit_Add_Panel"), "20000", "visible", "true")Then
			'--------------------------------------------------------------------------------------------------------------------------------
			If Fn_Web_UI_WebEdit_Operations("Fn_AWC_Object_Copy_To_Existing_Operation ","Set",obj_AWCTeamcenterHome, "wedt_Search_Edit_Add_Panel", sID) Then
				Reporter.ReportEvent micPass, "set [" & sID & "]  in Search edit box.", "Successfully set [" & sID &"] in Search edit box."
			Else
				Reporter.ReportEvent micFail, "set [" & sID & "] in Search edit box.", "Fail to set [" & sID & "] in Search edit box."
				ExitComponent
			End  If
			'--------------------------------------------------------------------------------------------------------------------------------	
		Else
			Reporter.ReportEvent micFail, "Search edit box does not exist.", "Search edit box does not exist."
			ExitComponent	
		End If
		'--------------------------------------------------------------------------------------------------------------------------------
		'Click on Search magnifier
		'--------------------------------------------------------------------------------------------------------------------------------
		If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation ","Click",obj_AWCTeamcenterHome,"wele_Search_Magnifire_Add_Panel","","","")=False Then
			Reporter.ReportEvent micFail, "Click on Search magnifier of Relate panel", "Fail to click on Search magnifier of Relate panel"
			ExitComponent
		Else
			Reporter.ReportEvent micPass, "Click on Search magnifier of Relate panel", "Successfully clicked on Search magnifier of Relate panel"
		End If
		Call Fn_AWC_ReadyStatusSync(1)
		'--------------------------------------------------------------------------------------------------------------------------------
		'Select Object from search result
		'--------------------------------------------------------------------------------------------------------------------------------
		Call Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation ", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_SearchResult_List_Listwithsummary"), "2", "innertext",sSecObject )
		If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation ","Click",obj_AWCTeamcenterHome,"wele_SearchResult_List_Listwithsummary","","","")= False Then
			Reporter.ReportEvent micFail, "Click on [ "&sSecObject&" ] from search result.", "Fail to click on [ "&sSecObject&" ] from search result."
			ExitComponent
		Else
			Reporter.ReportEvent micPass, "Click on [ "&sSecObject&" ] from search result.", "Successfully clicked on [ "&sSecObject&" ] from search result."
		End If
		Call Fn_AWC_ReadyStatusSync(1)
	End IF	
	'--------------------------------------------------------------------------------------------------------------------------------
	'Click on Save  button 
	'--------------------------------------------------------------------------------------------------------------------------------
	If Fn_Web_UI_WebElement_Operations("Fn_AWC_Object_Copy_To_Existing_Operation","Click",obj_AWCTeamcenterHome.WebElement("wele_Save_Copy_Existing"),"","","","") =False Then
		Reporter.ReportEvent micFail, "Click on [ Save  ] button ", "Fail to click on [ Save  ] button"
		ExitComponent
	Else
		Reporter.ReportEvent micPass, "Click on [ Save  ] button", "Successfully clicked on [ Save ] button"
	End If
	'--------------------------------------------------------------------------------------------------------------------------------
	'Verify notification message
	'--------------------------------------------------------------------------------------------------------------------------------
	If obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg").WaitProperty("visible","true") Then
		obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg").Click
		sStatusMesaage= Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "getroproperty", obj_AWCTeamcenterHome.WebElement("wele_ObjectCreationNotificationMsg"), "2", "innertext", "")
		If instr(lcase(sStatusMesaage),"is successful")>0 Then
			Reporter.ReportEvent micPass, "Verify Notification message", "Successfully verified notification message which is [ "&  sStatusMesaage  & " ]"
		Else
			Reporter.ReportEvent micFail, "Verify Notification message", "Fail to verify notification message ["&sStatusMesaage&"]"
			ExitComponent
		End If 
		obj_AWCTeamcenterHome.WebButton("wbtn_NotificationClose").Click
	End If
	Call Fn_AWC_ReadyStatusSync(2)
	'--------------------------------------------------------------------------------------------------------------------------------
	'Select Tab ->Section and select Table view
	'--------------------------------------------------------------------------------------------------------------------------------
	Call Fn_AWC_Object_Navigation_WorkArea_Panel_Operations(obj_AWCTeamcenterHome,"selectview_in_section","Table",sTabNm & "~" & sSecNm )	
	Call Fn_AWC_ReadyStatusSync(2)
	'--------------------------------------------------------------------------------------------------------------------------------
	'Verify added object
	'--------------------------------------------------------------------------------------------------------------------------------
	Call Fn_Web_UI_WebObject_Operations(" Fn_AWC_Object_Copy_To_Existing_Operation", "settoproperty", obj_AWCTeamcenterHome.WebElement("wele_attachments_Added_Table_2"), "2", "innertext",sSecObject )
	If Fn_Web_UI_WebObject_Operations("Fn_AWC_Object_Copy_To_Existing_Operation", "exist", obj_AWCTeamcenterHome.WebElement("wele_attachments_Added_Table_2"), "2", "", "")=False Then
		Reporter.ReportEvent micFail, "Verify [ "& sSecObject &" ] is added under " & sTabNm & "->" & sSecNm & " table list.", "Fail to verify [ "& sSecObject &" ] is added under " & sTabNm & "->" & sSecNm  & " table list."
		ExitComponent
	Else
		Reporter.ReportEvent micPass, "Verify [ "& sSecObject &" ] is added under " & sTabNm & "->" & sSecNm & " table list.", "Successfully verified that [ "& sSecObject &" ] is added under " & sTabNm & "->" & sSecNm & " table list."
	End  If	
	'--------------------------------------------------------------------------------------------------------------------------------
	'Click on Clear Selection
	'--------------------------------------------------------------------------------------------------------------------------------
	If obj_AWCTeamcenterHome.WebButton("wbtn_SelectAll_ClearSelections_ResultPanel").exist  Then
		Call Fn_WEB_UI_WebButton_Operations("", "click", obj_AWCTeamcenterHome, "wbtn_SelectAll_ClearSelections_ResultPanel","","","")	
	End If
	'--------------------------------------------------------------------------------------------------------------------------------
	'Click on workflow history tab
	'--------------------------------------------------------------------------------------------------------------------------------
	Call Fn_AWC_Common_Business_Verify_Properties_Operations(obj_AWCTeamcenterHome,"",gWorkflowHistory,"","","")
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	If Err.Number<> 0 Then
		Reporter.ReportEvent micFail, "Fn_AWC_Object_Copy_To_Existing_Operation", "Fail to perform [ Fn_AWC_Object_Copy_To_Existing_Operation ]  operation due to [ " & Err.Description & " ]"
		ExitComponent
	Else
		Reporter.ReportEvent micPass, "Fn_AWC_Object_Copy_To_Existing_Operation ", "Successfully performed [ Fn_AWC_Object_Copy_To_Existing_Operation ] operation "
		Fn_AWC_Object_Copy_To_Existing_Operation=True
	End If
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
	'Set object nothing
	Set obj_AWCTeamcenterHome=Nothing
	'-----------------------------------------------------------------------------------------------------------------------------------------------------------	
End Function

