Public Function Fn_Web_UI_WebObject_Operations(sFunctionName, sAction, objReferencePath, iTimeOut, sPropertyName, sPropertyValue)
	Err.Clear
	'Initially set function return value as False
	Fn_WEB_UI_WebObject_Operations = False
	
	'Set the default time out
	If iTimeOut = "" Then
		iTimeOut = DEFAULT_TIMEOUT
	Else
		iTimeOut = cInt(iTimeOut)
	End If
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
	Select Case lcase(sAction)
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to verify existance of Webobject
		Case "exist"
			Fn_Web_UI_WebObject_Operations = objReferencePath.Exist(iTimeOut)
			If Fn_Web_UI_WebObject_Operations Then
'				Reporter.ReportEvent micPass, "Existence of WebObject", "WebObject is exist"
				Fn_Web_UI_WebObject_Operations=True
			End If
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to verify non existance of Webobject
		Case "nonexist"
			Fn_Web_UI_WebObject_Operations = objReferencePath.Exist(iTimeOut)
			If Fn_Web_UI_WebObject_Operations =False Then
				'Reporter.ReportEvent micPass, "Existence of WebObject", "WebObject does not exist"
				Fn_Web_UI_WebObject_Operations=True
			Else
				Fn_Web_UI_WebObject_Operations=False
			End If
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to verify Webobject is enabled
		Case "enabled"
			'Verify if the reference path of object is exist
			If Fn_Web_UI_WebObject_Operations(sFunctionName, "Exist", objReferencePath, iTimeOut,"","") Then
				If objReferencePath.CheckProperty("Disabled",0) Then
					Reporter.ReportEvent micPass, "Existence and Enable the WebObject", "WebObject is exist and enabled"
					Fn_Web_UI_WebObject_Operations = True
				Else
					Reporter.ReportEvent micPass, "Existence and Enable the WebObject", "WebObject is not enabled"
					Fn_Web_UI_WebObject_Operations = True
				End If
			Else
				Reporter.ReportEvent micFail, "Existence and Enable the WebObject", "WebObject does not exist"
			End If
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to get the property value
		Case "getroproperty"
				Fn_Web_UI_WebObject_Operations = objReferencePath.GetROProperty(sPropertyName)
				Reporter.ReportEvent micPass, "Fetched  property value", "Sucessfully fetched  property value [ " & sPropertyName & " ] of object [ "& objReferencePath.ToString &" ]"
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to set the property value
		Case "settoproperty"
				objReferencePath.SetTOProperty sPropertyName,sPropertyValue
'				Reporter.ReportEvent micPass, "Set property value", "Sucessfully set property [ " & sPropertyName & " ] of object [ "& objReferencePath.ToString &" ] with value [ " & sPropertyValue & " ]"
				Fn_Web_UI_WebObject_Operations = True
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to CheckProperty  
		Case "notcheckproperty"
				Fn_Web_UI_WebObject_Operations=objReferencePath.CheckProperty(sPropertyName,sPropertyValue,iTimeOut)
				If Fn_Web_UI_WebObject_Operations=False Then
					Reporter.ReportEvent micPass, "Check property value", "Sucessfully check property [ " & sPropertyName & " ] of object [ "& objReferencePath.ToString &" ] with value [ " & sPropertyValue & " ]"
					Fn_Web_UI_WebObject_Operations = True
				Else
					Fn_Web_UI_WebObject_Operations=False
				End If	
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
			'Case to CheckProperty  
			Case "checkproperty"
					objReferencePath.CheckProperty sPropertyName,sPropertyValue,iTimeOut
					Reporter.ReportEvent micPass, "Check property value", "Sucessfully check property [ " & sPropertyName & " ] of object [ "& objReferencePath.ToString &" ] with value [ " & sPropertyValue & " ]"
					Fn_Web_UI_WebObject_Operations = True				
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -			
		Case "waituntilexist"
				objReferencePath.WaitProperty sPropertyName,sPropertyValue,iTimeOut
				Reporter.ReportEvent micPass, "Wait property value", "Sucessfully wait until property [ " & sPropertyName & " ] of object [ "& objReferencePath.ToString &" ] Achive value [ " & sPropertyValue & " ]"
				Fn_Web_UI_WebObject_Operations = True					
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to handle invalid request
		Case Else
			Reporter.ReportEvent micFail, "Fn_WEB_UI_WebObject_Operations", "No valid case was passed for function [ Fn_WEB_UI_WebObject_Operations ] "
	End Select
' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
End Function
