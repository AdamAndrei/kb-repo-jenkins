Public Function Fn_Web_UI_WebElement_Operations(sFunctionName,sAction,objPage,sWebElement,iXValue,iYValue,coMicButton)
  	Err.Clear
	  Dim objWebElement, sFuncLog,bResult
	   Dim WshShell
	Set WshShell = CreateObject("WScript.Shell")
	'Object Creation
	If sWebElement<>"" Then
		Set objWebElement = objPage.WebElement(sWebElement)
	Else
		Set objWebElement = objPage
	End If
	
	Fn_Web_UI_WebElement_Operations = False
	
'	IF WaitUntilExist(objWebElement, 60, 5)=False Then	
	If objWebElement.Exist=False Then
'		Reporter.ReportEvent micFail, "Existence of webelement", objWebElement.tostring & "webelement does not exist"
		Set objWebElement=Nothing
		Exit Function	
	End If
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	Select Case lcase(sAction)
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
		Case "click"
       		     If  iXValue <> "" AND iYValue <> "" Then
				'Click the mouse button at X,Y Co-ordinates
				objWebElement.Click iXValue, iYValue, coMicButton
				Fn_Web_UI_WebElement_Operations = True
			Else
				objWebElement.highlight
				objWebElement.Object.focus
				objWebElement.Click
				Fn_Web_UI_WebElement_Operations = True
			End If
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -		
		Case "clickext"
       		     If  iXValue <> "" AND iYValue <> "" Then
				'Click the mouse button at X,Y Co-ordinates
				objWebElement.Click iXValue, iYValue, coMicButton
				Fn_Web_UI_WebElement_Operations = True
			Else
				objWebElement.Object.focus
				objWebElement.Click
				Fn_Web_UI_WebElement_Operations = True
			End If
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -		
		Case "multiselect"
'		Set objDeviceReplay = CreateObject("Mercury.DeviceReplay")
				objWebElement.Object.focus
				WshShell.SendKeys "+"
				objWebElement.Click
				
				Fn_Web_UI_WebElement_Operations = True
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -			
		 Case "rightmousebutton"	
'		 		If  iXValue <> "" AND iYValue <> "" Then
				'Click the mouse button at X,Y Co-ordinates
				objWebElement.Object.focus
				objWebElement.highlight
'				objWebElement.Click 1,1,micRightButton
				WshShell.SendKeys "+{F10}"
				Fn_Web_UI_WebElement_Operations = True
'			    End If
	End Select
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	Set objWebElement = Nothing
End Function

