Public Function Fn_Web_UI_WebEdit_Operations(sFunctionName, sAction, objWebDialog, sWebEdit, sValue)
	Err.Clear
	'Declaring Variables
	Dim objWebEdit,objDeviceReplay

	'Initially set function return value as False
	Fn_Web_UI_WebEdit_Operations=False
	
	If sWebEdit<>"" Then
		Set objWebEdit = objWebDialog.WebEdit(sWebEdit)
	Else
		Set objWebEdit = objWebDialog
	End If
	
	'Verify WebEditBox object exists
	IF WaitUntilExist(objWebEdit, 60, 3)=False Then	
		'Report error/message when WebEditBox object is not exist
		Reporter.ReportEvent micFail, "Existence of Web Edit box", "WebEditBox does not exist"
		Set objWebEdit=Nothing
			
	End  If	
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	Select Case lcase(sAction)
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to set the value in WebEditBox
		Case "set","setnocheck"
			wait 1
			objWebEdit.Set " "
			wait 2
			objWebEdit.Set sValue
			wait 1
			If lcase(sAction)="set" Then
				objWebEdit.CheckProperty "value",sValue,2
				Reporter.ReportEvent micPass, "Check property value", "Sucessfully check property [ value ] of object [ "& objWebEdit.ToString &" ] with value [ " & sValue & " ]"
			End If
			Reporter.ReportEvent micPass, "Set Value in WebEditBox", "Successfully set the value [ " & Cstr(sValue) & " ] in WebEditBox [ " & objWebEdit.toString & " ]"	
			Fn_Web_UI_WebEdit_Operations = True
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -		
		'Case to set the value in WebEditBox
		Case "setext"
			wait 2
			objWebEdit.Set sValue
			wait 1
			objWebEdit.CheckProperty "value",sValue,2
			Reporter.ReportEvent micPass, "Check property value", "Sucessfully check property [ value ] of object an [ "& objWebEdit.ToString &" ] with value [ " & sValue & " ]"
			Reporter.ReportEvent micPass, "Set Value in WebEditBox", "Successfully set the value [ " & Cstr(sValue) & " ] in WebEditBox [ " & objWebEdit.toString & " ]"	
			Fn_Web_UI_WebEdit_Operations = True
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to set the value in WebEditBox using send string method
      	   Case "sendstring"
	        Set objDeviceReplay = CreateObject("Mercury.DeviceReplay")
			objWebEdit.Set ""
			objWebEdit.Object.focus
			objDeviceReplay.SendString sValue
			Wait 0, 300
			objDeviceReplay.PressKey 28
			
			Reporter.ReportEvent micPass, "Set Value in WebEditBox", "Successfully type the value [ " & Cstr(sValue) & " ] in WebEditBox [ " & objWebEdit.toString & " ]"
			Set objDeviceReplay =Nothing
			Fn_Web_UI_WebEdit_Operations = True
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to click in WebEditBox 
		Case "click"
			objWebEdit.highlight
			objWebEdit.Click
			wait 1
			Reporter.ReportEvent micPass, "Click in WebEditBox", "Successfully in WebEditBox "
			Fn_Web_UI_WebEdit_Operations = True
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
		'Case to set the value in WebEditBox
		Case "setsecure"
			'Verify WebEditBox object is enabled
			If Fn_Web_UI_WebObject_Operations("Fn_WEB_UI_WebEdit_Operations", "Enabled", objWebEdit,"","","") = False Then
				Reporter.ReportEvent micFail, "Existence of Web Edit box", "WebEditBox is not enabled"
				'Release Object
				Set objWebEdit = Nothing
				Fn_WEB_UI_WebEdit_Operations= False
				Exit Function
			End If
		
			objWebEdit.SetSecure sValue
'			objWebEdit.CheckProperty "value",sValue,2
'			Reporter.ReportEvent micPass, "Check property value", "Sucessfully check property [ value ] of object [ "& objWebEdit.ToString &" ] with value [ " & sValue & " ]"
			Reporter.ReportEvent micPass, "Set Value in WebEditBox", "Successfully set the value [ " & Cstr(sValue) & " ] in WebEditBox [ " & objWebEdit.toString & " ]"
			Fn_Web_UI_WebEdit_Operations = True
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
		Case Else
			Reporter.ReportEvent micFail, "Fn_WEB_UI_WebEdit_Operations", "[No k case] : No valid case was passed for function [ Fn_WEB_UI_WebEdit_Operations ]"
		' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	End Select
	' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	
	'Report any unexpected runtime error
	If Err.Number <> 0 Then
		Reporter.ReportEvent micFail, "Fn_Web_UI_WebEdit_Operations", "Fail To perform operation [ " & Cstr(sAction) & " ] due to error number [ " & Cstr(Err.Number) & " ] with error description [ " & Cstr(Err.Description) & " ]"
		Fn_WEB_UI_WebEdit_Operations = False
	End If
	
	'Release objects
	Set objWebEdit = Nothing
End Function
' - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
'Function Name		:	Fn_Web_UI_WebElement_Operations
'Description		:	Function to perform operations on Web Element.
'Parameters			:	1. sFunctionName
'						2. sAction
